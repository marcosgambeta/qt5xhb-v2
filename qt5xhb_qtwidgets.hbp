-hblib

-olib/${hb_plat}/${hb_comp}/hbqtwidgets5

-workdir=obj

-n
-w3
-gc3

-cpp

-isource/QtWidgets
-iinclude
-i${QT_INC_DIR}
#-i${QT_INC_DIR}/QtCore
#-i${QT_INC_DIR}/QtGui

-cflag=-frtti
-cflag=-fexceptions
{allmingw}-cflag=-mthreads
-cflag=-Wall
-cflag=-DUNICODE
-cflag=-DQT_DLL
-cflag=-DQT_NO_DEBUG
-cflag=-DQT_THREAD_SUPPORT
-cflag=-DQT_WIDGETS_LIB
-cflag=-Wno-deprecated-declarations

-plugin=qt5xhb_qtwidgets.hb

