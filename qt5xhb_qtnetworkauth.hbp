-hblib

-olib/${hb_plat}/${hb_comp}/hbqtnetworkauth5

-workdir=obj

-n
-w3
-gc3

-cpp

-isource/QtNetworkAuth
-iinclude
-i${QT_INC_DIR}

-cflag=-frtti
-cflag=-fexceptions
{allmingw}-cflag=-mthreads
-cflag=-Wall
-cflag=-DUNICODE
-cflag=-DQT_DLL
-cflag=-DQT_NO_DEBUG
-cflag=-DQT_THREAD_SUPPORT
-cflag=-DQT_NETWORKAUTH_LIB
-cflag=-Wno-deprecated-declarations

-plugin=qt5xhb_qtnetworkauth.hb

source/QtNetworkAuth/QAbstractOAuth.prg
source/QtNetworkAuth/QAbstractOAuth2.prg
source/QtNetworkAuth/QAbstractOAuthReplyHandler.prg
source/QtNetworkAuth/QOAuth1.prg
source/QtNetworkAuth/QOAuth1Signature.prg
source/QtNetworkAuth/QOAuth2AuthorizationCodeFlow.prg
source/QtNetworkAuth/QOAuthHttpServerReplyHandler.prg
source/QtNetworkAuth/QOAuthOobReplyHandler.prg
source/QtNetworkAuth/QtNetworkAuthVersion.cpp
